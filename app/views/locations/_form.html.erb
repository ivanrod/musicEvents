<%= form_for [@location] do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :city %>
<%= f.text_field :city %>
<%= f.label :latitud %>
<%= f.number_field :latitud %>
<%= f.label :longitud %>
<%= f.number_field :longitud %>
<br/>
<%= f.submit "New location" %>
<% end %>

<%= link_to("Cancel", locations_path) %>